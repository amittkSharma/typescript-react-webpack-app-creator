{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "strict": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
    "node_modules" // don't run on any code in the node_modules directory
  ]
}